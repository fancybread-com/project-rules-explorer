{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "outDir": "../out/test",
    "lib": ["ES2020"],
    "sourceMap": true,
    "rootDir": ".",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "noImplicitAny": false
  },
  "include": [
    "**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
